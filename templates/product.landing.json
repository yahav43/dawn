    "rich_text_kz3cXM": {
      "type": "rich-text",
      "blocks": {
        "text_7wamNy": {
          "type": "text",
          "settings": {
            "text": "<p><strong>אם את מרגישה שאת רחוקה מזה – את במקום הנכון.</strong></p><p><br/>StressLess הוא הכלי שלך להחזיר את עצמך לידיים שלך.</p>"
          }
        }
      },
      "block_order": [
        "text_7wamNy"
      ],
      "settings": {
        "desktop_content_position": "center",
        "content_alignment": "center",
        "color_scheme": "",
        "full_width": true,
