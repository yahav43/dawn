        "heading_gEcVrh": {
          "type": "heading",
          "settings": {
            "heading": "<strong>לא צריך לעשות יוגה או לשבת במדיטציה ארוכה בישיבת לוטוס עם הידיים על הברכיים </strong>👌🏻🧘🏾‍♀️",
            "heading_size": "h0"
